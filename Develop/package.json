{
  "name": "JATE",
  "version": "1.0.0",
  "description": "PWA text-editor",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run start:client\" \"npm run start:server\"",
    "start:client": "webpack-dev-server",
    "build:client": "webpack --mode production",
    "start:server": "npm run server --prefix server",
    "server": "nodemon server.js --ignore client",
    "install": "npm install --prefix client",
    "client": "npm start --prefix client"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "if-env": "^1.0.4"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
